<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rdnational.syntechx.syntech_x">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- <uses-permission android:name="android.permission.CALL_PHONE" /> -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/syntechx_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/syntechx_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash_Screen"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventDetails"
            android:theme="@style/Theme.Swipe.Back" />
        <activity
            android:name=".SocialMediaLinks"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Swipe.Back" />
        <activity
            android:name=".CommitteeHeadRecyclerView"
            android:theme="@style/Theme.Swipe.Back" />
        <activity android:name=".Sponsors"
            android:theme="@style/Theme.Swipe.Back">

        </activity>
    </application>

</manifest>